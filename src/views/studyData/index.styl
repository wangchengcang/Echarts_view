    .view_center{
        width 100.25rem;
        height 100%;
        margin-left 1rem;
        margin-right 1.25rem;
        // float left;
        position absolute;
        top 0rem;
        left 50rem;
        .center_top{
            width calc(100% - 4.25rem);
            height 66.5rem;
            padding 2.125rem 2rem 0rem 2.25rem;
            margin-bottom 1rem;
            background-color rgba(0, 0, 0, .2);
            .center_top_oneDom{
                height 18.875rem;
                .border_class_deiv{
                    width 46.625rem;
