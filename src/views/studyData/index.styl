.stidyData{
    width 100%;
    height 100%;
    position relative;
    .view_left{
        width 48.125rem;
        height calc(100% - 2.25rem);
        padding 2.25rem 0rem 0rem 1.875rem;
        background-color rgba(0, 0, 0, .2);
        // float left;
        position absolute;
        top 0rem;
        left 0rem;
        .left_one_Dom{
