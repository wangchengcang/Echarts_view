            width calc(100% - 4.375rem);
            height 46.5rem;
            padding 1.875rem 2.25rem 0px 2.125rem;
            margin-bottom 1.5rem;
            background-color rgba(0, 0, 0, .2);
            position relative;
            .view_right_son{
                width calc(50% - .125rem);
                height 32.625rem;
                float left;
                .son{
                    width 100%;
                    height calc(50% - .125rem);
                    text-align center;
                    .right_num_dom{
                        margin-right 4.125rem;
                        display inline-block;
                    }
                    .right_num_dom:last-child{
                        margin-right 0;
                    }
                    .subunit{
                        display block;
                    }
                }
            }
        }
        .right_center_Dom{
            width calc(100% - 1.25rem);
            height 31.625rem;
            padding 2.375rem 0rem 3.125rem 1.25rem;
            margin-bottom 1.25rem;
            background-color rgba(0, 0, 0, .2);
        }
        .right_bottom_Dom{
            width calc(100% - 1.25rem);
            height 31.625rem;
            padding 2.375rem 0rem 3.125rem 1.25rem;
            background-color rgba(0, 0, 0, .2);
        }
        .right_View_line{
            width 100%;
            height calc(100% - 2.75rem);
        }
